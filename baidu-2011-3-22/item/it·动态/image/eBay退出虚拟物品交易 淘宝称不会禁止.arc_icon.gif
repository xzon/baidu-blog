	/**
	 *	生成板块排行榜html
	 */
	function refreshPlateRank(indexes, plates) {
		// right bar
		// <td bgcolor="#ff9999" width="%s"></td>;
		// <td style="padding-left: 5px;">%s</td>;
		// left bar
		// <td align="right" style="padding-right: 5px;"></td>	
		//
		
		// 将指数数组和板块数组合并
		var datas = indexes.concat(plates);
		
		var htmlbuf = [];	//板块排行榜的html	
		htmlbuf.push('<table cellspacing="0" cellpadding="0" border="0" width="187" style="font-size: 12px; line-height: 12px;"><tbody>');
		// 指数部分
		for(var i=0; i<datas.length; i++) {
			var name = datas[i]['STYLENAME'];
			var percent = datas[i]['PERCENT'];
			// table
			tablebuf = [];
			tablebuf.push('<table cellspacing="0" cellpadding="0" border="0" width="100%" style="height: 11px; line-height: 11px;"><tbody><tr>');
if(percent>0) {
				tablebuf.push('<td bgcolor="#ff9999" width="'+(parseInt(percent/0.15*100)<1 ? 1 : parseInt(percent/0.15*100))+'%"></td>');
				tablebuf.push('<td style="padding-left: 5px;">'+(percent*100).toFixed(2)+'%</td>');
			} else {
				tablebuf.push('<td align="right" style="padding-right: 5px;">'+(percent*100).toFixed(2)+'%</td>');
				tablebuf.push('<td height="11" bgcolor="#95cf87" width="'+(parseInt(percent/0.15*-100)<2 ? 2 : parseInt(percent/0.15*-100))+'%"></td>');		
			}			
			tablebuf.push('</tr></tbody></table>');
			
			htmlbuf.push('<tr>');
			if(percent>0) {
				htmlbuf.push('<td height="23" align="right" width="93" valign="top" style="border-right: 1px solid rgb(114, 113, 113);">');
				htmlbuf.push('<div style="margin-right: 8px; vertical-align: top;">'+name+'</div>');
				htmlbuf.push('</td><td height="23" align="left" width="93" valign="top">');
				// add <table>
				htmlbuf.push(tablebuf.join(''));
				htmlbuf.push('</td>');
			} else {
				htmlbuf.push('<td height="23" align="right" width="93" valign="top" style="border-right: 1px solid rgb(114, 113, 113);">');
				// add <table>
				htmlbuf.push(tablebuf.join(''));
				htmlbuf.push('</td><td height="23" align="left" width="93" valign="top">');
				htmlbuf.push('<div style="margin-left: 8px; vertical-align: top;">'+name+'</div></td>');	
			}
			htmlbuf.push('</tr>');
			
			// 指数与板块之间的分隔线
			if(i == 1) {	
				htmlbuf.push('<tr><td height="1" style="border-right: 1px solid rgb(114, 113, 113);"><span style="margin: 0px 0px 10px 0px;" class="dotLine"></span></td>');
				htmlbuf.push('<td height="1"><span style="margin: 0px 0px 10px 0px;" class="dotLine"></span></td></tr>');
			}
			if(i == 6) {
				htmlbuf.push('<tr><td height="5" style="border-right: 1px solid rgb(114, 113, 113);"><span style="margin: 0px 0px 10px 0px;" class="dotLine"></span></td>');
				htmlbuf.push('<td height="5"><span style="margin: 0px 0px 10px 0px;" class="dotLine"></span></td></tr>');
			}
		}
		htmlbuf.push('</tbody></table>');
		return htmlbuf.join('');
	}
